{% extends 'AppBundle::base.html.twig' %}
{% block title %}Rodrigo Santellan - Ups{% endblock %}
{% block body %}
<!-- START SEPARATOR  -->
<div id="separator">
	<div class="btop-1px"></div>
	<div class="container">
		<!-- start separator -->
		<div class="sixteen columns">
			<h4 class="page-title">{% trans %}rsantellan_site_error_title{% endtrans %}</h4>
		</div><!-- sixteen columns -->
	</div><!-- .container -->
	<div class="bbottom-1px"></div>
</div><!-- #separator -->
<!-- END SEPARATOR -->
<!-- START PAGE WRAPPER -->
<div class="container main-wrapper">
	<div id="main-content" class="sixteen columns">
		<div class="row remove-bottom clearfix">
			<img alt="" src="{{ 'web/images/100_3863.JPG' | mImage(283, 262, 'rce', true) }}" class="border alignright" title="Align Right" width="200" />
			{% trans %}rsantellan_site_error_message{% endtrans %} 
		</div>
	</div><!-- main-content -->
</div><!-- .container -->
<!-- END BLOG WRAPPER -->
{% endblock %}
