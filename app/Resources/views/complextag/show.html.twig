{% extends 'base.html.twig' %}

{% block body %}
    <h1>Complextag</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ complexTag.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ complexTag.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ complexTag.description }}</td>
            </tr>
            <tr>
                <th>Orden</th>
                <td>{{ complexTag.orden }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_complextag_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_complextag_edit', { 'id': complexTag.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
